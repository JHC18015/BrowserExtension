{
  "manifest_version": 3,
  "name": "Magic Image Tool",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "web_accessible_resources": [{ 
    "resources": ["sexybeast.JPG"],
    "matches": ["https://*/*"]
}],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*",
        "https://*/*"
      ]
    }
  ]
}
